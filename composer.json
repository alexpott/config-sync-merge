{
    "name": "alexpott/config-sync-merge",
    "description": "Merge multiple Drupal config directories into a single sync storage",
    "type": "library",
    "license": "GPL-2.0+",
    "authors": [
        {
            "name": "Alex Pott",
            "email": "alex.a.pott@googlemail.com"
        }
    ],
    "require": {
        "drupal/core": "^8.3",
        "drupal/config_filter": "*"
    },
    "autoload": {
        "psr-4": {
            "alexpott\\ConfigSyncMerge\\": "src/",
            "Drupal\\config_sync_merge\\": "src/",
            "Drupal\\config_filter\\": "vendor/drupal/config_filter/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "alexpott\\ConfigSyncMerge\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "mikey179/vfsStream": "^1.6"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "drupal/config_filter",
                "version": "1.0.0",
                "type": "drupal-module",
                "dist": {
                    "url": "https://ftp.drupal.org/files/projects/config_filter-8.x-1.0-rc1.zip",
                    "type": "zip"
                }
            }
        }
    ],
    "scripts": {
        "phpunit": "./vendor/bin/phpunit"
    }
}
